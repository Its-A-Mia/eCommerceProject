generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          String @default(cuid()) @id
  title       String
  price       Decimal @db.Decimal(10,2)
  category    String
  description String
  createdAt   DateTime @default(now()) @map(name: "created_at") 
  updatedAt   DateTime @updatedAt
}